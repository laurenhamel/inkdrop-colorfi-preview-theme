{
  "name": "colorfi-preview",
  "description": "Colorfi preview theme for Inkdrop Markdown Editor",
  "version": "0.0.0",
  "theme": "preview",
  "repository": "https://github.com/laurenhamel/inkdrop-colorfi-preview-theme",
  "license": "MIT",
  "scripts": {
